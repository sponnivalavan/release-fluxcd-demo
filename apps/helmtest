kind: HelmRepository
metadata:
  annotations:
    source-file: helmtest
  name: helmsource
  namespace: flux-system
spec:
  accessFrom:
    namespaceSelectors:
    - matchLabels:
        environment: flux-system
  interval: 1m0s
  url: https://github.com/stefanprodan/podinfo
